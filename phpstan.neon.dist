parameters:
  paths:
    - src
  level: 9
  tmpDir: report/phpstan
  ignoreErrors:
    - '#T of#'
    - '#mixed#'
    - '#template type#'
    - '#ReflectionType#'
    - '#generic interface#'
    - '#is used zero times#'
    - '#expects class-string#'
    - '#value type specified#'
    - '#not specify its types: T#'
  reportUnmatchedIgnoredErrors: false
